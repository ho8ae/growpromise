{
  "expo": {
    "name": "kids-plan",
    "slug": "kids-plan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "kidsplan",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-location",
        {
          "accuracy": "best",
          "foregroundService": true,
          "requestAlwaysAuthorization": true,
          "requestWhenInUseAuthorization": true,
          "useActivity": true,
          "locationAlwaysAndWhenInUsePermission": "앱이 위치 정보를 사용하여 약속 장소를 확인합니다."
        }
      ],
      [
        "expo-image-picker",
        {
          "mediaTypes": "photo",
          "allowsEditing": true,
          "aspect": [4, 3],
          "quality": 1
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f7541177-1d8c-456d-8f63-3d3fbcf26f31"
      }
    },
    "owner": "low_k"
  }
}
