{
  "expo": {
    "name": "growpromise",
    "slug": "kids-plan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "growpromise",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.low-k.growpromise",
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": false,
        "UIStatusBarStyle": "UIStatusBarStyleDefault",
        "NSCameraUsageDescription": "약속 인증을 위해 카메라 접근이 필요합니다.",
        "NSPhotoLibraryUsageDescription": "사진을 선택하기 위해 갤러리 접근이 필요합니다.",
        "NSMicrophoneUsageDescription": "알림음을 위해 마이크 접근이 필요합니다."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.low_k.growpromise"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.584178392628-loms8bh6egsfr1q2fl9om43i9i51530g"
        }
      ],
      ["expo-apple-authentication"],
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "mediaTypes": "photo",
          "allowsEditing": true,
          "aspect": [4, 3],
          "quality": 1
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f7541177-1d8c-456d-8f63-3d3fbcf26f31"
      }
    },
    "owner": "low_k"
  }
}
